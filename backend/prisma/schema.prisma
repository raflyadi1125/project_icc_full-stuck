generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model client_tb {
  id String @id @default (uuid())
  name String
  email String @unique
  job String
  isActive Boolean
  rate Decimal @db.Decimal(10,2)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
